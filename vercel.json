{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/book",
      "destination": "/backend/api/book.js"
    },
    {
      "source": "/api/approveBooking",
      "destination": "/backend/api/approveBooking.js"
    },
    {
      "source": "/api/declineBooking",
      "destination": "/backend/api/declineBooking.js"
    },
    {
      "source": "/api/getBookings",
      "destination": "/backend/api/getBookings.js"
    },
      {
      "source": "/api/appointments",
      "destination": "/backend/api/appointments.js"
    },
      {
      "source": "/api/admin-login",
      "destination": "/backend/api/admin-login.js"
    },
         {
      "source": "/api/approve",
      "destination": "/backend/api/approve.js"
    },
     {
      "source": "/api/decline",
      "destination": "/backend/api/decline.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
